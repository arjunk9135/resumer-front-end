{
    "version": 2,
    "builds": [
      {
       "src": "dist/public/**/*",
      "use": "@vercel/static",
        "config": {
          "distDir": "dist/public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/public/index.html"
      }
    ],
    "installCommand": "npm install && npm install vite",
    "buildCommand": "npm run vercel-build"
  }
  