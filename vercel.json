{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/public/$1"
      },
      {
        "src": "/",
        "dest": "/dist/public/index.html"
      }
    ],
    "installCommand": "npm install && npm install vite",
    "buildCommand": "npm run vercel-build"
  }
  